div(class="modal" id="delete-image")
  div(class="modal-content")
    div#delete-img-form
      h4 Delete this image
      span Are you sure you want to delete this image
      span#question-mark ?
    div.affirm-or-negat
      button(type="button" class="btn" onclick="postDelete()") Delete
      button(type="button" class="btn" onclick="clickCancelDelete()") Cancel
div.grid
  div.grid-sizer
  each val in images
    div.grid-item
      a(href= val.url target="_blank")
        img(onload="addClass(this)" src= val.urlmedium onerror="imgError(this)" class="omg-img")
      if currentpage != '/profile'
        div.info
          div.avatar
            img(src= val.avatar)
            a(href= "/user/" + val.userid)= val.username
      else
        div.info
          div(class="delete-img" id= val.imageid)
            i(class="fas fa-trash-alt")
            span(onclick= "openDelete(this)")  Delete
div.page-nav
  if currentpagenum > 1 && currentpagenum <= numpages
    a(href= currentpage + '?page=' + prevpagenum) Previous
  if currentpagenum < numpages && currentpagenum >= 1
    a(href= currentpage + '?page=' + nextpagenum) Next
